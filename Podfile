# Your iOS deployment target must be set to 13.0 or higher.
platform :ios, '16.4'

target 'Example' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

# Pods for Example
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'
  pod 'Courier_iOS'

end

target 'CourierService' do
  use_frameworks!
  pod 'Courier_iOS'
end
